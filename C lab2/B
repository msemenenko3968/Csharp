using System;
using System.Collections.Generic;
using System.Text;

namespace OOP
{
    class B : A
    {
        private int _d;

        public B(int a, int b, int d) : base(a, b)
        {
            _d = d;
        }
        public B()
        {
        }

        public int c2
        {
            get
            {

                return _d;
            }
            set
            {
                _d = value;
            }
        }
    }
}